---
interface Props {
    class: string;
}

const { class: classname } = Astro.props;
---

<svg
    class={classname}
    xmlns="http://www.w3.org/2000/svg"
    fill="none"
    stroke="currentColor"
    stroke-linecap="round"
    stroke-linejoin="round"
    stroke-width="2"
    class="w-5 icon icon-tabler icon-tabler-chevron-right"
    viewBox="0 0 24 24"
    ><path stroke="none" d="M0 0h24v24H0z"></path><path d="m9 6 6 6-6 6"
    ></path></svg
>
